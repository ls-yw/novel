<div class="weui-flex nav-top" style="opacity: 1;position: relative;">
    <div class="weui-flex__item"><img src="data:image/png;base64," class="back"></div>
    <div class="weui-flex__item title">申请收录</div>
    <div class="weui-flex__item home"><a href="/"><i class="fa fa-home"></i></a></div>
</div>
<div class="weui-tab__bd-item weui-tab__bd-item--active">
    <div class="weui-panel weui-panel_access index-list">
        <form class="form-horizontal" method="post">
        <div class="weui-panel__hd"></div>
        <div class="weui-panel__bd">
            <div class="weui-cell">
                <div class="weui-cell__hd"><label class="weui-label">小说名称</label></div>
                <div class="weui-cell__bd">
                    <input class="weui-input" id="book_name" name="book_name" type="text" placeholder="请输入小说名称">
                </div>
            </div>
            <div class="weui-cell" style="border-bottom: 1px solid #e5e5e5;">
                <div class="weui-cell__hd">
                    <label class="weui-label">作者</label>
                </div>
                <div class="weui-cell__bd">
                    <input class="weui-input" id="author" type="text" name="author" placeholder="请输入作者">
                </div>
            </div>
        </div>
        <div class="weui-cells__tips green" style="font-size: 12px"><?=$memberConfig['apply_book'] ?? ''?></div>
        <div class="weui-panel__bd" style="padding: 50px 10px;">
            <button type="submit" class="weui-btn weui-btn_primary">提 交</button>
        </div>
        </form>
    </div>
</div>